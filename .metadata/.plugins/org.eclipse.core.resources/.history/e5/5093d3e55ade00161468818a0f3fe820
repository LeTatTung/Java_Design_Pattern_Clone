package com.letattung.hexagonal.domain;

public class LotteryTicketId {

	private static volatile int numAllocated;
	private final int id;
	
	public LotteryTicketId() {
		this.id = numAllocated + 1;
		numAllocated ++;
	}
	
	public LotteryTicketId(int id){
		this.id = id;
	}
	
	public int getId(){
		return id;
	}
	
	
}
